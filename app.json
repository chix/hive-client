{
  "expo": {
    "name": "hive",
    "description": "Hive client app.",
    "slug": "hive-client",
    "privacy": "unlisted",
    "platforms": [
      "android"
    ],
    "version": "1.0.0",
    "githubUrl": "https://github.com/chix/hive-client",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "primaryColor":"#ffffff",
    "notification": {
      "icon": "./assets/images/icon.png",
      "androidMode": "default"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cz.martinkuric.hive"
    },
    "android": {
      "package": "cz.martinkuric.hive",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "splash": {
        "backgroundColor": "#ffffff"
      }
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff",
      "translucent": false
    }
  }
}
